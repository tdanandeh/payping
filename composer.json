{
  "name": "Tohidda/payping",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tohid Danandeh",
      "email": "livedars@yahoo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PayPing\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "require": {
    "guzzlehttp/guzzle": "^7.2",
    "ext-openssl": "*",
    "ext-json": "*"
  },
  "extra": {
